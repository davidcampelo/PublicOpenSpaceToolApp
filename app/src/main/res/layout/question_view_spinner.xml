<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

    <TextView
        android:id="@+id/MultipleQuestionView_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:textAppearanceMedium"
        android:text=""
        android:layout_centerHorizontal="false" />

    <Spinner
        android:id="@+id/MultipleQuestionView_listOptions"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/MultipleQuestionView_title" />

    <LinearLayout
        android:id="@+id/MultipleQuestionView_containerOptions"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/MultipleQuestionView_listOptions">
    </LinearLayout>
</RelativeLayout>
